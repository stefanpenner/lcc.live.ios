# Matchfile for code signing

git_url(ENV["MATCH_GIT_URL"]) # Git repo for storing certificates and profiles
storage_mode("git")

type("appstore") # Can be appstore, adhoc, development, enterprise

app_identifier(["lcc.live"])
username(ENV["APPLE_ID"])
team_id(ENV["TEAM_ID"])

